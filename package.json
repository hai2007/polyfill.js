  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hai2007/polyfill.js/issues"
  },
  "homepage": "https://github.com/hai2007/polyfill.js#readme",
  "devDependencies": {
    "@hai2007/nodejs": "^1.0.0",
    "rollup": "^1.17.0",
    "rollup-plugin-commonjs": "^10.0.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "uglify-js": "^3.10.2"
  },
  "dependencies": {
    "@hai2007/tool": "^0.6.2"
  }
}
