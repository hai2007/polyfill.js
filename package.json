    "uglifyjs": "uglifyjs ./dist/polyfill.js -m -o ./dist/polyfill.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hai2007/polyfill.js.git"
  },
  "keywords": [
    "hai2007",
    "polyfill"
  ],
